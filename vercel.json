{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/kpis", "dest": "/kpis.html" },
    { "src": "/knowledgebase", "dest": "/knowledgebase.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
