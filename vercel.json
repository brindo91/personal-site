{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    { "source": "/about.html", "destination": "/about", "permanent": true },
    { "source": "/kpis.html", "destination": "/kpis", "permanent": true }
  ]
}
